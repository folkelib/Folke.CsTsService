<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Create Typescript API Client classes from MVC 6 controllers</Description>
    <VersionPrefix>3.0</VersionPrefix>
    <Authors>Sidoine</Authors>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <AssemblyName>Folke.CsTsService</AssemblyName>
    <PackageId>Folke.CsTsService</PackageId>
    <PackageTags>typescript;services</PackageTags>
    <PackageProjectUrl>https://github.com/folkelib/Folke.CsTsService</PackageProjectUrl>
    <PackageLicenseUrl>http://opensource.org/licenses/MIT</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
    <PackageReference Include="System.Runtime.Serialization.Primitives" Version="4.3.0" />
  </ItemGroup>
</Project>
